<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.examhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_3"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity android:name=".ChooseSubjectActivity"></activity>

        <activity android:name=".ChooseIntentionActivity"></activity>

        <activity android:name=".AddTask"></activity>

        <activity android:name=".DataBaseActivity"></activity>

        <activity android:name=".AnsweringActivity"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".DataBaseDisplayActivity"></activity>
    </application>

</manifest>